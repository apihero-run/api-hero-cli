{
	"name": "apihero",
	"version": "1.0.9",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/apihero-run/api-hero-cli"
	},
	"keywords": [
		"API Hero",
		"API",
		"CLI"
	],
	"author": "Matt Aitken",
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/apihero-run/api-hero-cli/issues"
	},
	"homepage": "https://github.com/apihero-run/api-hero-cli",
	"bin": {
		"apihero": "./bin/run"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"clean": "rimraf dist",
		"build": "tsc && chmod +x dist/index.js",
		"start": "npm run build && dist/index.js",
		"pretest": "npm run build",
		"test": "xo && ava",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"files": [
		"bin/run",
		"bin/run.cmd",
		"dist/**/*"
	],
	"dependencies": {
		"@boost/cli": "^4.0.0",
		"@npmcli/promise-spawn": "^3.0.0",
		"dotenv": "^16.0.1",
		"ink": "^3.2.0",
		"ink-big-text": "^1.2.0",
		"ink-gradient": "^2.0.0",
		"ink-spinner": "^4.0.3",
		"nanoid": "^3.0.0",
		"node-fetch": "^2.6.7",
		"posthog-node": "^2.1.0",
		"react": "^17.0.2",
		"tiny-invariant": "^1.2.0",
		"ts-node": "^10.9.1",
		"zod": "^3.18.0"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/ink-big-text": "^1.2.1",
		"@types/ink-gradient": "^2.0.1",
		"@types/node-fetch": "^2.6.2",
		"@types/pacote": "^11.1.5",
		"@types/react": "^18.0.17",
		"ava": "^4.3.1",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^2.1.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.4",
		"xo": "^0.39.1"
	}
}
